@phdthesis{khang_studying_2024,
	type = {Thesis},
	title = {Studying strapi {CMS} framework and its practicalities in the future of application development},
	copyright = {Attribution-NonCommercial 4.0 International},
	url = {https://epub.vgu.edu.vn/handle/dlibvgu/1854},
	abstract = {It is important for organizations to have content management systems (CMS) that are strong, flexible, and scalable so they can grow as the business does. Because web creation is always changing, this is the case. Strapi is well-known in this field, it is an open-source content management system (CMS) that have an excellent user interface. With any front-end framework, they can still use a useful tool to keep track of information. This essay talks about Strapi’s structure and features, as well as how it can be used to make app creation better right now. We look at Strapi’s headless method, which keeps the frontend and the backend for managing material separate. These tools, like as React, Vue.js, and Angular, facilitate the process of joining. The paper discusses Strapi’s API-first design. Developers may create dynamic and captivating user experiences using RESTful. In addition, we assess the efficacy of Strapi’s tools in facilitating modifications. Because it comes with so many plug-ins and content types thatcan be changed, the CMS can be tailored to each project’s needs.},
	language = {en},
	urldate = {2025-01-06},
	author = {Khang, Nguyen Phu},
	year = {2024},
	note = {Accepted: 2024-11-15T04:20:47Z},
	file = {Snapshot:C\:\\Users\\Janis\\Zotero\\storage\\5TFTL3IF\\1854.html:text/html},
}

